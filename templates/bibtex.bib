\mode{comment}{CCC "%" "\n"}%
%
\define{EQ}{ #1 = {{#2}},
}%
\define{ES}{ #1 = "#2",
}%
\define{E}{ #1 = #2,
}%
%
@@$bibEntryType${$bibid$,
$if(authors)$\ES{author   }{$authors$}$endif$%
$if(title)$\EQ{title    }{$title$}$endif$%
$if(proceedings)$\ES{booktitle}{$proceedingsTex$}$endif$%
$if(series)$\ES{series   }{$seriesTex$}$endif$%
$if(school)$\ES{school   }{$school$}$endif$%
$if(journal)$\EQ{journal  }{$journalTex$}$endif$%
$if(volume)$\ES{volume   }{$volume$}$endif$%
$if(year)$\E{year     }{$year$}$endif$%
$if(pages)$\ES{pages    }{$pages$}$endif$%
$if(publisher)$\ES{publisher}{$publisherTex$}$endif$%
$if(submitted)$\ES{note     }{Submitted.}$endif$%
}%
